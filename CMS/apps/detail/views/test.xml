<?xml version="1.0" encoding="utf-8" ?>
<tempalte>
    <tempalte-get>
        <filter type="subtree">
            <vlan xmlns="http://www.huawei.com/netconf/vrp" content-version="1.0" format-version="1.0">
                <vlans>
                </vlans>
            </vlan>
        </filter>
    </tempalte-get>
    <tempalte-filter>
    <filter type="subtree">
        <vlan xmlns="http://www.huawei.com/netconf/vrp" content-version="1.0" format-version="1.0">
            <vlans>
                <vlan>
                    <vlanId>${vlanId}</vlanId>
                </vlan>
            </vlans>
        </vlan>
    </filter>
    </tempalte-filter>

    <tempalte-create>
    <config>
        <vlan xmlns="http://www.huawei.com/netconf/vrp" content-version="1.0" format-version="1.0">
            <vlans>
                <vlan>
                    <vlanId>${vlanId}</vlanId>
                    <vlanType>common</vlanType>
                    <vlanName>${vlanName}</vlanName>
                    <vlanDesc>${vlanDesc}</vlanDesc>
                    <adminStatus>up</adminStatus>
                    <subVlans/>
                    <superVlan/>
                    <vlanif>
                        <ifName></ifName>
                        <cfgBand>1000</cfgBand>
                        <dampTime>0</dampTime>
                    </vlanif>
                    <muxVlan>
                        <separateVlan/>
                        <groupVlans/>
                    </muxVlan>
                </vlan>
            </vlans>
        </vlan>
    </config>
    </tempalte-create>

    <tempalte-update>
    <config>
    <vlan xmlns="http://www.huawei.com/netconf/vrp" content-version="1.0" format-version="1.0">
        <vlans>
            <vlan>
                <vlanId>${vlanId}</vlanId>
                <vlanType>common</vlanType>
                <vlanName>${vlanName}</vlanName>
                <vlanDesc>${vlanDesc}</vlanDesc>
                <adminStatus>up</adminStatus>
                <subVlans/>
                <superVlan/>
                <vlanif>
                    <ifName></ifName>
                    <cfgBand>1000</cfgBand>
                    <dampTime>0</dampTime>
                </vlanif>
                <muxVlan>
                    <separateVlan/>
                    <groupVlans/>
                </muxVlan>
            </vlan>
        </vlans>
    </vlan>
    </config>
    </tempalte-update>

    <tempalte-delete>
    <config>
        <vlan xmlns="http://www.huawei.com/netconf/vrp" content-version="1.0" format-version="1.0">
            <vlans>
                <vlan operation="delete">
                    <vlanId>${vlanId}</vlanId>
                    <vlanif operation="delete">
                        <ifName>Vlanif${vlanId}</ifName>
                        <cfgBand>1000</cfgBand>
                        <dampTime>0</dampTime>
                    </vlanif>
                </vlan>
            </vlans>
        </vlan>
    </config>
    </tempalte-delete>
</tempalte>